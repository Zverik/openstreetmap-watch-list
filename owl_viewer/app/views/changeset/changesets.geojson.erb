{ "type": "FeatureCollection",
  "features": [
<% if @changesets %>
<% @changesets.each_with_index do |changeset, index| %>
{ "type": "Feature",
  "geometry": <%= changeset.geojson.html_safe %>,
  "properties": {
    "changeset_id": <%= changeset.id %>,
    "created_at": "<%= changeset.created_at %>",
    "user_id": "<%= changeset.user.id %>",
    "user_name": "<%= changeset.user.name %>"
  }
}<% if index < @changesets.size - 1 %>,<% end %>
<% end %>
<% end %>
  ],
  "crs": {
    "type": "OGC",
    "properties": {
      "urn": "urn:ogc:def:crs:OGC:1.3:CRS84"
    }
  }
}
