{ "type": "FeatureCollection",
  "features": [
<% @changesets.each do |changeset| %>
{ "type": "Feature",
  "geometry": <%= changeset.geojson.html_safe %>,
  "properties": {"changeset_id": <%= changeset.id %>}
  },
<% end %>
  ],
  "crs": {
    "type": "OGC",
    "properties": {
      "urn": "urn:ogc:def:crs:OGC:1.3:CRS84"
    }
  }
}
